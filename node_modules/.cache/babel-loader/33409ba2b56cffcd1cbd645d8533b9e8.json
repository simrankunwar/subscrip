{"ast":null,"code":"import db from \"../firebaseDb\";\nexport default {\n  name: \"EditMy\",\n  data() {\n    return {\n      subscription: {}\n    };\n  },\n  methods: {\n    addSubscription() {\n      if (this.subscription.name.length !== 0 && this.subscription.description.length !== 0) {\n        db.collection(\"subscriptions\").add(this.subscription).then(() => {\n          alert(\"Subscription successfully created!\");\n          this.subscription.name = \"\";\n          this.subscription.description = \"\";\n          this.subscription.amount = \"\";\n          this.subscription.frequency = \"\";\n        });\n        this.error = true;\n        this.errorMsg = \"Please fill out the title and description\";\n        setTimeout(() => {\n          this.error = false;\n        }, 5000);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiEA,OAAOA,EAAC,MAAO,eAAe;AAE9B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,CAAC;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,IACE,IAAI,CAACF,YAAY,CAACF,IAAI,CAACK,MAAK,KAAM,KAClC,IAAI,CAACH,YAAY,CAACI,WAAW,CAACD,MAAK,KAAM,GACzC;QACAN,EAAE,CAACQ,UAAU,CAAC,eAAe,EAC1BC,GAAG,CAAC,IAAI,CAACN,YAAY,EACrBO,IAAI,CAAC,MAAM;UACVC,KAAK,CAAC,oCAAoC,CAAC;UAC3C,IAAI,CAACR,YAAY,CAACF,IAAG,GAAI,EAAE;UAC3B,IAAI,CAACE,YAAY,CAACI,WAAU,GAAI,EAAE;UAClC,IAAI,CAACJ,YAAY,CAACS,MAAK,GAAI,EAAE;UAC7B,IAAI,CAACT,YAAY,CAACU,SAAQ,GAAI,EAAE;QAClC,CAAC,CAAC;QACJ,IAAI,CAACC,KAAI,GAAI,IAAI;QACjB,IAAI,CAACC,QAAO,GAAI,2CAA2C;QAC3DC,UAAU,CAAC,MAAM;UACf,IAAI,CAACF,KAAI,GAAI,KAAK;QACpB,CAAC,EAAE,IAAI,CAAC;MACV;IACF;EACF;AACF,CAAC","names":["db","name","data","subscription","methods","addSubscription","length","description","collection","add","then","alert","amount","frequency","error","errorMsg","setTimeout"],"sourceRoot":"","sources":["/Users/simrankunwar/subscriptions-app/src/components/EditMy.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"row justify-content-center\">\n      <div class=\"col-md-5\">\n        <h3 class=\"text-center\">Update Subscription</h3>\n        <form @submit.prevent=\"onFormSubmit\">\n          <div class=\"form-group\">\n            <label>Name</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              v-model=\"subscription.name\"\n              required\n            />\n          </div>\n\n          <div class=\"form-group\">\n            <label>Description</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              v-model=\"subscription.description\"\n              required\n            />\n          </div>\n\n          <div class=\"form-group\">\n            <label>Amount</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              v-model=\"subscription.amount\"\n              required\n            />\n          </div>\n\n          <div class=\"form-group\">\n            <label>Frequency</label>\n            <select\n              id=\"frequency\"\n              class=\"form-control\"\n              v-model=\"subscription.frequency\"\n              required\n            >\n              <option value=\"weekly\">Weekly</option>\n              <option value=\"bimonthly\">Bi-monthly</option>\n              <option value=\"monthly\">Monthly</option>\n              <option value=\"quarterly\">Quarterly</option>\n              <option value=\"biannually\">Bi-annually</option>\n              <option value=\"annually\">Annually</option>\n            </select>\n          </div>\n\n          <div class=\"form-group\">\n            <button class=\"btn btn-primary btn-block\" @click=\"addSubscription\">\n              Add Subscription\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport db from \"../firebaseDb\";\n\nexport default {\n  name: \"EditMy\",\n  data() {\n    return {\n      subscription: {},\n    };\n  },\n  methods: {\n    addSubscription() {\n      if (\n        this.subscription.name.length !== 0 &&\n        this.subscription.description.length !== 0\n      ) {\n        db.collection(\"subscriptions\")\n          .add(this.subscription)\n          .then(() => {\n            alert(\"Subscription successfully created!\");\n            this.subscription.name = \"\";\n            this.subscription.description = \"\";\n            this.subscription.amount = \"\";\n            this.subscription.frequency = \"\";\n          });\n        this.error = true;\n        this.errorMsg = \"Please fill out the title and description\";\n        setTimeout(() => {\n          this.error = false;\n        }, 5000);\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}